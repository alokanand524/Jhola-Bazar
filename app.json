{
  "expo": {
    "name": "Jhola Bazar",
    "slug": "jhola-Bazar",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/jhola-bazar.png",
    "scheme": "jholabazar",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,


    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jholabazar"
    },

    "android": {
      "package": "com.coolboy14.jholabazar",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/jhola-bazar1.png",
        "backgroundColor": "#FFCC00"
      },

      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },

    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/jhola-bazar1.png"
    },

    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "This app needs access to location to show nearby stores and delivery options."
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true
    },

    "extra": {
      "router": {},
      "eas": {
        "projectId": "381be625-a928-43a3-8619-c8a92eedc237"
      }
    },

    "owner": "coolboy14"
  }
}
