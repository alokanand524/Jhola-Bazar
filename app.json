{
  "expo": {
    "name": "Jhola Bazar",
    "slug": "jhola-bazar",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/jhola-bazar.png",
    "scheme": "jholabazar",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jholabazar"
    },

    "android": {
      "package": "com.coolboy14.jholabazar",
      "versionCode": 2,
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE"
      ]
    },

    "web": {
      "bundler": "metro",
      "output": "static"
    },

    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "This app needs access to location to show nearby stores and delivery options."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/jhola-bazar.png",
          "color": "#ffffff",
          "sounds": ["./assets/notification-sound.wav"]
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true
    },

    "extra": {
      "eas": {
        "projectId": "1d32e2b2-88ae-4ac1-8521-459d6934f8ad"
      }
    },

    "owner": "coolboy14"
  }
}
