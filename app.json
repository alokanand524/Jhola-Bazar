{
  "expo": {
    "name": "Jhola-Bazar",                
    "slug": "jhola-bazar",                
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/jhola-bazar.png",   
    "scheme": "jholabazar",               
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alok524.jholabazar"   
    },

    "android": {
      "package": "com.alok524.jholabazar",           
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/jhola-bazar.png",  
        "backgroundColor": "#FFCC00"
      },
      "edgeToEdgeEnabled": true
    },

    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/jhola-bazar.png"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/jhola-bazar.png",
          "imageWidth": 200,
          "resizeMode": "contain",                     
          "backgroundColor": "#ffffff"
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true
    },

    "extra": {
      "router": {},
      "eas": {
        "projectId": "89b2babb-199c-4e18-a7a3-0c172da970a5"
      }
    }
  }
}